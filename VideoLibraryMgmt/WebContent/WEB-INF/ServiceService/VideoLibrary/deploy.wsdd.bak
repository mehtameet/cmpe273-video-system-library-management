        <parameter qname="pns:purchaseDetails" xmlns:pns="http://VideoLibrary" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <fault name="MalformedException" qname="fns:fault" xmlns:fns="http://VideoLibrary" class="exception.MalformedException" type="tns:MalformedException" xmlns:tns="http://exception"/>
      </operation>
      <operation name="addUpdateMoviesDetails" qname="operNS:addUpdateMoviesDetails" xmlns:operNS="http://VideoLibrary" returnQName="retNS:addUpdateMoviesDetailsReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:columnId" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:movieDetails" xmlns:pns="http://VideoLibrary" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
