      </operation>
      <operation name="deleteUser" qname="operNS:deleteUser" xmlns:operNS="http://VideoLibrary" returnQName="retNS:deleteUserReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userId" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="issueMovies" qname="operNS:issueMovies" xmlns:operNS="http://VideoLibrary" returnQName="retNS:issueMoviesReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
