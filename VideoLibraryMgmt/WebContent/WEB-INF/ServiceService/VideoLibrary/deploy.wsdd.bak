        <parameter qname="pns:password" xmlns:pns="http://VideoLibrary" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="searchUsers" qname="operNS:searchUsers" xmlns:operNS="http://VideoLibrary" returnQName="retNS:searchUsersReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:Customer" xmlns:rtns="http://entity" soapAction="" >
        <parameter qname="pns:searchBy" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:searchValue" xmlns:pns="http://VideoLibrary" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lowerLimit" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
