      <operation name="getUsersByMovieId" qname="operNS:getUsersByMovieId" xmlns:operNS="http://VideoLibrary" returnQName="retNS:getUsersByMovieIdReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
