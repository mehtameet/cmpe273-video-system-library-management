        <parameter qname="pns:amountPaid" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:transactionDate" xmlns:pns="http://VideoLibrary" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="searchMovies" qname="operNS:searchMovies" xmlns:operNS="http://VideoLibrary" returnQName="retNS:searchMoviesReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:Movie" xmlns:rtns="http://entity" soapAction="" >
        <parameter qname="pns:searchBy" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
