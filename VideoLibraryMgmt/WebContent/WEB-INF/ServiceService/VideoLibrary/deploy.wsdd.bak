      </operation>
      <operation name="getAccountDetails" qname="operNS:getAccountDetails" xmlns:operNS="http://VideoLibrary" returnQName="retNS:getAccountDetailsReturn" xmlns:retNS="http://VideoLibrary" returnType="rtns:AccountDetails" xmlns:rtns="http://entity" soapAction="" >
        <parameter qname="pns:membershipId" xmlns:pns="http://VideoLibrary" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="issueMovies getMovieCategoryList returnMovies getUserDetails searchUsers addUpdateUserDetails main deleteMovies getUsersByMovieId addUpdateMoviesDetails getMovieDetails searchMovies deleteUser signIn getAccountDetails"/>

