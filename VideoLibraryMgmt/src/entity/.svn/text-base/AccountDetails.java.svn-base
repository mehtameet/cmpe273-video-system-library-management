package entity;

public class AccountDetails {
	private String plan;
	private int noOfMovies;
	MovieAccountDetail[] movieAccountDetail;
	
	public String getPlan() {
		return plan;
	}

	public void setPlan(String plan) {
		this.plan = plan;
	}

	public int getNoOfMovies() {
		return noOfMovies;
	}

	public void setNoOfMovies(int noOfMovies) {
		this.noOfMovies = noOfMovies;
	}

	public MovieAccountDetail[] getMovieAccountDetail() {
		return movieAccountDetail;
	}

	public void setMovieAccountDetail(MovieAccountDetail[] movieAccountDetail) {
		this.movieAccountDetail = movieAccountDetail;
	}
}
