package entity;

public class SimpleCustomer {

	private int simpleCustomerId;
	private int userId;
	private int numberOfMoviesIssued;
	
	public int getSimpleCustomerId() {
		return simpleCustomerId;
	}
	public void setSimpleCustomerId(int simpleCustomerId) {
		this.simpleCustomerId = simpleCustomerId;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public int getNumberOfMoviesIssued() {
		return numberOfMoviesIssued;
	}
	public void setNumberOfMoviesIssued(int numberOfMoviesIssued) {
		this.numberOfMoviesIssued = numberOfMoviesIssued;
	}
	
	
}
